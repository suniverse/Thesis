	Keywords = {instabilities, magnetic fields, methods: numerical, MHD, plasmas, Stars: Pulsars: General},
	Month = may,
	Pages = {520--530},
	Title = {Diocotron instability in pulsar electrospheres. {I}. {Linear} analysis},
