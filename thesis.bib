	Journal = {The Astrophysical Journal Letters},
	Keywords = {stars: evolution, stars: magnetars, stars: magnetic field, stars: neutron},
	Month = oct,
	Pages = {L162--L167},
	Title = {Broadband {Study} with {Suzaku} of the {Magnetar} {Class}},
	Url = {http://adsabs.harvard.edu/abs/2010ApJ...722L.162E},
	Urldate = {2017-04-26},
