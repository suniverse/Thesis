@article{li_plastic_2015,
	Abstract = {Magnetar flares generate Alfv{\'e}n waves bouncing in the closed
magnetosphere with energy up to \~{} \{10\}46 erg. We show
that on a timescale of 10 ms the waves are transmitted into the star and
form a compressed packet of high energy density. This packet strongly
shears the stellar crust and initiates a plastic flow, heating the crust
and melting it hundreds of meters below the surface. A fraction of the
deposited plastic heat is eventually conducted to the stellar surface,
contributing to the surface afterglow months to years after the flare. A
large fraction of heat is lost to neutrino emission or conducted into
the core of the neutron star.},
	Author = {Li, Xinyu and Beloborodov, Andrei M.},
	Doi = {10.1088/0004-637X/815/1/25},
	File = {NASA/ADS Full Text PDF:/home/alex/Zotero/storage/9JXZIJJK/Li and Beloborodov - 2015 - Plastic Damping of Alfv{\'e}n Waves in Magnetar Flares.pdf:application/pdf},
	Issn = {0004-637X},
	Journal = {The Astrophysical Journal},
	Keywords = {dense matter, magnetic fields, stars: magnetars, stars: neutron, WAVES},
	Month = dec,
	Pages = {25},
	Title = {Plastic {Damping} of {Alfv{\'e}n} {Waves} in {Magnetar} {Flares} and {Delayed} {Afterglow} {Emission}},
	Url = {http://adsabs.harvard.edu/abs/2015ApJ...815...25L},
	Urldate = {2017-03-09},
