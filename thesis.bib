	Issn = {0004-637X},
	Journal = {The Astrophysical Journal},
	Keywords = {Cosmic Rays, Crab Nebula, Diffuse Radiation, Galactic Radiation, Galactic Structure, GAMMA RAYS, PULSARS, Sas, Spaceborne Telescopes, supernova remnants, X Ray Telescopes},
	Month = may,
	Pages = {163--182},
	Title = {High-energy gamma-ray results from the second small astronomy satellite},
	Url = {http://adsabs.harvard.edu/abs/1975ApJ...198..163F},
