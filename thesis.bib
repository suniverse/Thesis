	Keywords = {Angular Momentum, Electron Emission, MAGNETIC STARS, PAIR PRODUCTION, PULSAR MAGNETOSPHERES, PULSARS, Spin Reduction, STELLAR MODELS, STELLAR ROTATION, Torque},
	Month = nov,
