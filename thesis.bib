	Issn = {0035-8711},
	Journal = {Monthly Notices of the Royal Astronomical Society},
	Keywords = {acceleration of particles, Magnetic reconnection, methods: numerical, outflows, pulsars: general, stars: winds},
	Month = mar,
	Pages = {606--619},
	Title = {Particle acceleration in axisymmetric pulsar current sheets},
