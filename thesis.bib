	Title = {Discovery of {Luminous} {Pulsed} {Hard} {X}-{Ray} {Emission} from {Anomalous} {X}-{Ray} {Pulsars} 1RXS {J}1708-4009, 4U 0142+61, and 1E 2259+586 by {INTEGRAL} and {RXTE}},
	Url = {http://adsabs.harvard.edu/abs/2006ApJ...645..556K},
