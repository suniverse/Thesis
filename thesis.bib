@article{beloborodov_magnetar_2016,
	Abstract = {We examine four candidate mechanisms that could explain the high surface temperatures of magnetars. (1) Heat flux from the liquid core heated by ambipolar diffusion. It could sustain the observed surface luminosity \$L\_s{\textbackslash}approx 10{\textasciicircum}\{35\}\$ erg/s if core heating offsets neutrino cooling at a temperature \$T\_\{core\}{\textgreater}6{\textbackslash}times 10{\textasciicircum}8\$ K. This scenario is viable if the core magnetic field exceeds \$10{\textasciicircum}\{16\}\$ G and the heat-blanketing envelope of the magnetar has a light element composition. We find however that the lifetime of such a hot core should be shorter than the typical observed lifetime of magnetars. (2) Mechanical dissipation in the solid crust. This heating can be quasi-steady, powered by gradual (or frequent) crustal yielding to magnetic stresses. We show that it obeys a strong upper limit. As long as the crustal stresses are fostered by the field evolution in the core or Hall drift in the crust, mechanical heating is insufficient to sustain persistent \$L\_s{\textbackslash}approx 10{\textasciicircum}\{35\}\$ erg/s. The surface luminosity is increased in an alternative scenario of mechanical deformations triggered by external magnetospheric flares. (3) Ohmic dissipation in the crust, in volume or current sheets. This mechanism is inefficient because of the high conductivity of the crust. Only extreme magnetic configurations with crustal fields \$B{\textgreater}10{\textasciicircum}\{16\}\$ G varying on a 100 meter scale could provide \$L\_s{\textbackslash}approx 10{\textasciicircum}\{35\}\$ erg/s. (4) Bombardment of the stellar surface by particles accelerated in the magnetosphere. This mechanism produces hot spots on magnetars. Observations of transient magnetars show evidence for external heating.},
	Author = {Beloborodov, Andrei M. and Li, Xinyu},
	File = {arXiv.org Snapshot:/home/alex/Zotero/storage/UFM2WFIA/1605.html:text/html;Beloborodov_Li_2016_Magnetar heating.pdf:/home/alex/Zotero/storage/MBC3N6R9/Beloborodov_Li_2016_Magnetar heating.pdf:application/pdf},
	Journal = {arXiv:1605.09077 [astro-ph]},
