@article{duncan_formation_1992,
	Abstract = {Neutron stars with unusually strong magnetic dipole fields  B\_dipole {\textasciitilde}
10{\textasciicircum}14 - 10{\textasciicircum}15 G, can form when conditions for efficient helical dynamo
action are met during the first few seconds after gravitational
collapse.  Such high-field neutron stars, "magnetars," initially rotate
with short periods {\textasciitilde} 1 ms, but quickly lose most of their rotational
energy via magnetic braking, giving a large energy boost to the
associated supernova explosion.  Several mechanisms unique to magnetars
can plausibly generate large ({\textasciitilde} 1000 km/s) recoil velocities.  These
include anisotropic neutrino emission, core rotational instability and
fragmentation, and/or anisotropic magnetic winds.

Magnetars are relatively difficult to detect because they drop below the
radio death line faster than ordinary pulsars, and because they probably
do not remain bound in binary systems.  We conjecture that their main
observational signature is gamma-ray bursts powered by their vast
reservoirs of magnetic energy.  If they acquire large recoils, most
magnetars are unbound from the Galaxy or reside in an extended, weakly
bound Galactic corona.  There is evidence that the soft gamma repeaters
are young magnetars.

Finally, we note that a convective dynamo can also generate a very
strong dipole field after the merger of a neutron star binary, but only
if the merged star survives for as long as {\textasciitilde} 10-100 ms.},
	Author = {Duncan, Robert C. and Thompson, Christopher},
	Doi = {10.1086/186413},
	File = {Duncan_Thompson_1992_Formation of very strongly magnetized neutron stars - Implications for.pdf:/home/alex/Zotero/storage/GAEKJMBH/Duncan_Thompson_1992_Formation of very strongly magnetized neutron stars - Implications for.pdf:application/pdf},
	Issn = {0004-637X},
	Journal = {The Astrophysical Journal Letters},
	Keywords = {Dynamo Theory, GAMMA RAY BURSTS, Gravitational Collapse, NEUTRON STARS, Star Formation, STELLAR MAGNETIC FIELDS, STELLAR ROTATION, Supernovae},
	Month = jun,
	Pages = {L9--L13},
	Title = {Formation of very strongly magnetized neutron stars - {Implications} for gamma-ray bursts},
	Url = {http://adsabs.harvard.edu/abs/1992ApJ...392L...9D},
	Urldate = {2016-10-07},
	Volume = {392},
	Year = {1992},
	Bdsk-Url-1 = {http://adsabs.harvard.edu/abs/1992ApJ...392L...9D},
	Bdsk-Url-2 = {https://doi.org/10.1086/186413}}

@article{cheng_energetic_1986,
	Abstract = {Very rapidly spinning magnetized neutron stars such as the Crab and Vela pulsars are strong $\gamma$-ray sources with photon energies extending to 1012eV and possibly beyond. These special pulsars must contain extremely ultrarelativistic particle accelerators. In this paper the authors consider the origin and structure of particle accelerators in the outer magnetosphere of pulsars characterized by large spindown energy loss rates. The discussion is concerned with the class of (mainly young) pulsars which can sustain pair production in otherwise charge-depleted regions of the outer magnetosphere. The resulting electrostatic accelerating region ("gap") is expected to exist far from the neutron star, where a corotating magnetosphere would be rotating at speeds a significant fraction of c. The size and location of this gap supports the production of energetic radiation beams of quite different character from the usual coherent microwave emission of radio pulsars.},
	Author = {Cheng, K. S. and Ho, C. and Ruderman, M.},
	Doi = {10.1086/163829},
	File = {Cheng et al_1986_Energetic radiation from rapidly spinning pulsars.pdf:/home/alex/Zotero/storage/VQZ884DV/Cheng et al_1986_Energetic radiation from rapidly spinning pulsars.pdf:application/pdf},
	Issn = {0004-637X},
	Journal = {The Astrophysical Journal},
	Keywords = {ELECTRIC FIELDS, PAIR PRODUCTION, PARTICLE ACCELERATION, PULSAR MAGNETOSPHERES, PULSARS, STELLAR ATMOSPHERES, STELLAR RADIATION, STELLAR ROTATION, STELLAR STRUCTURE},
	Month = jan,
	Pages = {500--539},
	Title = {Energetic radiation from rapidly spinning pulsars. {I} - {Outer} magnetosphere gaps. {II} - {VELA} and {Crab}},
	Url = {http://adsabs.harvard.edu/abs/1986ApJ...300..500C},
	Urldate = {2013-06-06},
